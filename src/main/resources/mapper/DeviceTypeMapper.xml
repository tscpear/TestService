<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.service.apiTest.dom.mapper.DeviceTypeMapper">

    <select id="getDeviceTypeList" resultType="java.lang.Integer">
        select type from device_type where device = #{device} and user_id = #{userId}
    </select>

</mapper>